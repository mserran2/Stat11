<h1>Listing surveys</h1>

<table>
  <tr>
    <th>Class year</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @surveys.each do |survey| %>
  <tr>
    <td><%= survey.class_year %></td>
    <td><%= link_to 'Show', survey_path(Base64.encode64((survey.id*17).to_s)) %></td>
